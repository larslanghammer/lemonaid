<mvc:View
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="com.sap.mentors.lemonaid.controller.Admin">

	<IconTabBar
		id="idIconTabBarMulti"
		expanded="{device>/isNoPhone}"
		select="onTabSelect"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter id="AdminHealth" icon="sap-icon://stethoscope" text="{i18n>health}" key="Admin-Health" />
			<IconTabFilter id="AdminTwitter" icon="sap-icon://chain-link" text="{i18n>twitter}" key="Admin-Twitter" />
            <IconTabFilter id="AdminTable" icon="sap-icon://chain-link" text="Table" key="Admin-Table" />
		</items>
	</IconTabBar>

</mvc:View>
