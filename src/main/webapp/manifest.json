{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "com.sap.mentors.lemonaid",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.36.2"
		},
		"dataSources": {
			"odata": {
				"uri": "/odata.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			},
			"user": {
				"uri": "/userapi/currentUser",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"handleValidation": true,
		"rootView": {
			"viewName": "com.sap.mentors.lemonaid.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resourceRoots": {
			"openui5.googlemaps": "./util/openui5_googlemaps/",
			"google.maps": "openui5/googlemaps/loadScripts"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.mentors.lemonaid.i18n.i18n"
				}
			},
			"user": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "user",
				"settings": {
          "defaultBindingMode": "TwoWay",
          "headers":{
            "Expires": "-1",
            "Pragma": "no-cache",
            "Cache-control": "no-cache"
          }
				}
			},
			"": {
				"dataSource": "odata",
                "type": "com.sap.mentors.lemonaid.model.ODataModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
          "json": true,
          "defaultOperationMode": "Client",
            "headers":{
            "Expires": "-1",
            "Pragma": "no-cache",
            "Cache-control": "no-cache"
          }
				}
			}
		},
			"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.mentors.lemonaid.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [{
				"pattern": "",
				"name": "Main",
				"target": "main"
			},{
				"pattern": "Mentors",
				"name": "Mentors",
				"target": "mentors"
			},{
				"pattern": "Mentor/{Id}",
				"name": "Mentor",
				"target": "mentor"
			},{
				"pattern": "MentorAddition",
				"name": "MentorAddition",
				"target": "mentorAddition"
			},{
				"pattern": "Events/:Id:",
				"name": "Events",
				"target": "events"
			},{
				"pattern": "Content",
				"name": "Content",
				"target": "content"
			},{
				"pattern": "SocialMedia",
				"name": "SocialMedia",
				"target": "socialMedia"
			},{
				"pattern": "ImportExport",
				"name": "ImportExport",
				"target": "importExport"
            },{
				"pattern": "Admin",
				"name": "Admin",
				"target": "admin"
            },{
				"pattern": "Admin/Health",
				"name": "Admin-Health",
				"target": "adminHealth"
            },{
				"pattern": "Admin/Twitter",
				"name": "Admin-Twitter",
				"target": "adminTwitter"
            },{
				"pattern": "Admin/Table",
				"name": "Admin-Table",
				"target": "adminTable"
            },{
				"pattern": "Help",
				"name": "Help",
				"target": "help"
            },{
				"pattern": "Help/Faq",
				"name": "Help-Faq",
				"target": "helpFaq"
            },{
				"pattern": "Help/About",
				"name": "Help-About",
				"target": "helpAbout"
			}],
			"targets": {
				"main": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"mentors": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "MentorList",
					"controlId": "Mentors",
					"controlAggregation": "content"
				},
				"mentor": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "MentorDetails",
					"controlId": "Mentor",
					"controlAggregation": "content"
				},
				"mentorAddition": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "MentorAddition",
					"controlId": "MentorAddition",
					"controlAggregation": "content"
				},
				"events": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "EventList",
					"controlId": "Events",
					"controlAggregation": "content"
				},
				"content": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "ContentList",
					"controlId": "Content",
					"controlAggregation": "content"
				},
				"socialMedia": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "SocialMedia",
					"controlId": "SocialMedia",
					"controlAggregation": "content"
				},
				"importExport": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "ImportExport",
					"controlId": "ImportExport",
					"controlAggregation": "content"
				},
                "admin": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "Admin",
					"controlId": "Admin",
					"controlAggregation": "content"
				},
                "adminHealth": {
					"parent": "admin",
					"viewPath": "com.sap.mentors.lemonaid.view.Admin",
					"viewName": "Health",
					"controlId": "AdminHealth",
					"controlAggregation": "content"
				},
                "adminTwitter": {
					"parent": "admin",
					"viewPath": "com.sap.mentors.lemonaid.view.Admin",
					"viewName": "Twitter",
					"controlId": "AdminTwitter",
					"controlAggregation": "content"
				},
				 "adminTable": {
					"parent": "admin",
					"viewPath": "com.sap.mentors.lemonaid.view.Admin",
					"viewName": "UpdatedTable",
					"controlId": "AdminTable",
					"controlAggregation": "content"
				},
               "help": {
					"parent": "main",
					"viewPath": "com.sap.mentors.lemonaid.view",
					"viewName": "Help",
					"controlId": "Help",
					"controlAggregation": "content"
				},
                "helpFaq": {
					"parent": "help",
					"viewPath": "com.sap.mentors.lemonaid.view.Help",
					"viewName": "Faq",
					"controlId": "HelpFaq",
					"controlAggregation": "content"
				},
                "helpAbout": {
					"parent": "help",
					"viewPath": "com.sap.mentors.lemonaid.view.Help",
					"viewName": "About",
					"controlId": "HelpAbout",
					"controlAggregation": "content"
				}

			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}
